{
  "compilerOptions": {
    "module": "ESNext",
    "target": "ES6",
    "lib": ["es2017", "dom"],
    "moduleResolution": "node",
    "outDir": "./static/js",
    "rootDir": "./typescript",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["typescript/**/*"]
}